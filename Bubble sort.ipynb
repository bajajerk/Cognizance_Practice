   "source": [
    "# Implementation of Bubble Sort\n",
    "l=[4,2,1,6,5,-1]\n",
    "n=len(l)\n",
    "for j in range (n):\n",
    "    for i in range (0,n-1):\n",
    "         if(l[i]>l[i+1]):\n",
    "                 t=l[i]\n",
    "                 l[i]=l[i+1]\n",
    "                 l[i+1]=t              \n",
    "       \n",
    "l"
   ]
